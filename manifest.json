{
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions":[
    "activeTab",
    "scripting"
  ],
  "background":{
    "service_worker":"background.js"
  },
  "action": {
    "default_popup": "extensionInfo.html",
    "default_icon": "icon.png"
  },
  "content_scripts":[
    {
      "matches": ["https://*/*"],
      "js":["content.js"]
    }
  ]
}
